    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "echarts": "^6.0.0",
    "element-plus": "^2.5.6",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vant": "^4.8.6",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
