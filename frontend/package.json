    "eslint-plugin-vue": "^9.22.0",
    "happy-dom": "^20.3.7",
    "jsdom": "^27.4.0",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.5",
    "vitest": "^4.0.18",
    "vue-tsc": "^2.0.6"
