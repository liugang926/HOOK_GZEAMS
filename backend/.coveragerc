# backend/.coveragerc

[run]
source = apps
omit =
    */migrations/*
    */tests/*
    */conftest.py
    */venv/*
    */__pycache__/*
branch = true

[report]
precision = 2
show_missing = true
skip_covered = false
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == '__main__':
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
